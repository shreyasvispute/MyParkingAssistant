<div class="container">
  <form method="POST" id="listBooking" action="/listings/createListing">
    {{#if hasErrors}}
      <div class="alert alert-danger error" role="alert">
        {{error}}
      </div>
    {{/if}}
    {{! <div class="alert alert-danger error" id="uiError" role="alert">
</div> }}
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="startDate">Start date</label>
        <input
          type="date"
          class="form-control"
          id="startDate"
          name="startDate"
        />
        <label for="endDate">End date</label>
        <input type="date" class="form-control" id="endDate" name="endDate" />
      </div>
      <div class="form-group col-md-6">
        <label for="startTime">Start time</label>
        <select id="startTime" name="startTime" class="form-control mr-sm-2">
          <option selected>Select Start Time</option>
          {{#each startTimeVal}}
            <option>{{this}}</option>
          {{/each}}

        </select>
        <label for="endTime">End time</label>
        <select id="endTime" name="endTime" class="form-control mr-sm-2">
          <option selected>Select End Time</option>
          {{#each endTimeVal}}
            <option>{{this}}</option>
          {{/each}}
        </select>
      </div>
      <div class="form-group col-md-6">
        <label for="price">Hourly Price</label>
        <input
          type="number"
          min="1"
          class="form-control"
          id="price"
          name="price"
        />
      </div>
    </div>
    <button
      type="submit"
      id="btnCreateListing"
      class="btn btn-primary"
    >Create</button>
  </form>
  <div class="alert alert-danger error" id="uiError" role="alert">
  </div>
</div>